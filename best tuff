                local durationBox = Instance.new("TextBox", pspawner)
                durationBox.Size = UDim2.new(0,80,0,28)
                durationBox.Position = UDim2.new(0,140,0,py-2)
                durationBox.Text = "10"
                durationBox.ClearTextOnFocus = false
